@model List<Agenda>
@{
    ViewData["Title"] = "Lista";
}
<h1>@ViewData["Title"]</h1>
<table width="467" border="2px">
    <capition>Agendamentos solicitados</capition>
    <thead>
        <tr>
        <th width="220">Nome</th>
        <th width="120">Animal</th>
        <th width="127">Servi√ßo</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Agenda item in Model)
        {
            <tr>
                <td>@item.nome</td>
                <td>@item.animal</td>
                <td>@item.servico</td>
            </tr>
        }
    </tbody>

</table>
<style>
    table{
        background-color: lightcoral;
    }
</style>
<br>
<a asp-action="Agendamentos">Novo agendamento</a>